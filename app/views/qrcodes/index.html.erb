<div class="container">
<p id="notice abcd"><%= notice %></p>
<h1>Listing Qrcodes</h1>
<strong class="abcd"><%= link_to 'New Qrcode', new_qrcode_path %></strong>

    <% @qrcodes.each do |qrcode| %>
      <div class="row">
       <div class="col s12 m6">
         <div class="card blue-grey darken-1">
           <div class="card-content white-text">
             <span class="card-title"><%= qrcode.link %></span>
           </div>
           <div class="card-action">
             <%= link_to 'Show', qrcode %>
             <%= link_to 'Edit', edit_qrcode_path(qrcode) %>
             <%= link_to 'Destroy', qrcode, method: :delete, data: { confirm: 'Are you sure?' } %>
           </div>
         </div>
       </div>
     </div>
    <% end %>




</div>
